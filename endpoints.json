{
	"User": {
		"token" : {
			"base_url": "request-token",
			"method": "POST",
			"payload": [
				{
					"name": "username",
					"type": "str",
					"required": true 
				},
				{
					"name": "password",
					"type": "str",
					"required": true 
				},
				{
					"name": "callback",
					"type": "str",
					"required": false 
				}
			]
		},

		"credits" : {
			"base_url": "quantidade-creditos",
			"method": "GET",
			"authenticated": true
		}
	},
	"Person": {
		"find" : {
			"base_url": "pessoas/{person}",
			"method": "GET",
			"authenticated": true,
			"url_params": [
				{
					"id": "{person}",
					"name": "id",
					"pattern": null,
					"type": "int"
				}
			]
		}
	}
}
